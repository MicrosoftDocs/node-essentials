# The endpoint URL for your Azure Cosmos DB account
COSMOS_ENDPOINT=<https://your-cosmos-account.documents.azure.com:443/>

# The primary key for your Azure Cosmos DB account
COSMOS_KEY=<your-cosmos-primary-key>

# The name of your Azure Cosmos DB database
COSMOS_DATABASE_NAME=<your-database-name>

# The name of your Azure Cosmos DB container
COSMOS_CONTAINER_NAME=<your-container-name>